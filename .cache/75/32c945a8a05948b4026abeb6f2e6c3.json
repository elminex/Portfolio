ion: absolute;\n        bottom: 16px;\n        left: 50%;\n        transform: translateX(-50%);\n    }\n}\n\n.nav {\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    z-index: 20;\n    background-color: rgba(0, 0, 0, .8);\n\n    &__container {\n        color: #fff;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n\n    &__list {\n        list-style-type: none;\n        padding: 0;\n    }\n\n    &__item {\n        display: inline-block;\n\n        &-link {\n            color: #fff;\n            text-decoration: none;\n        }\n    }\n}\n\n.about {\n    &__container {\n        display: flex;\n        background-color: white;\n    }\n\n    &__photo {\n        display: block;\n        width: 100%;\n        object-fit: cover;\n\n        &-wrapper {\n            flex: 1;\n        }\n    }\n\n    &__info {\n        flex: 2;\n    }\n}\n\n.skills {\n    &__container {\n        background-color: white;\n    }\n    &__list {\n        width: 100%;\n        list-style-type: none;\n        padding: 0;\n        display: flex;\n        flex-wrap: wrap;\n        justify-content: space-between;\n    }\n\n    &__item {\n        flex: 1;\n        height: 100px;\n        background: {\n            repeat: no-repeat;\n            position: center;\n            size: contain;\n        }\n        &.html {\n            background-image: url(\"../images/HTML5_Logo.svg\");\n        }\n        &.css {\n            background-image: url(\"../images/CSS3_logo_and_wordmark.svg\");\n        }\n        &.js {\n            background-image: url(\"../images/Unofficial_JavaScript_logo_2.svg\");\n        }\n        &.rwd {\n            background-image: url(\"../images/512px-Responsive_Web_Design_Logo.svg.png\");\n        }\n        &.bootstrap {\n            background-image: url(\"../images/Boostrap_logo.svg.png\");\n        }\n        &.git {\n            background-image: url(\"../images/Git-Logo-2Color.png\");\n        }\n        &.sass {\n            background-image: url(\"../images/sass-logo/Sass\\ Logo\\ -\\ Seal\\ \\(Color\\).eps\");\n        }\n        &.react {\n            background-image: url(\"../images/1024px-React-icon.svg.png\");\n        }\n    }\n\n    &__logo {\n        padding: 8px;\n        width: 100%;\n        display: block;\n        height: 100%;\n    }\n}\n\n.projects {\n    &__container {\n        background-color: white;\n    }\n    &__list {\n        width: 100%;\n        list-style-type: none;\n        padding: 0;\n        display: flex;\n        flex-wrap: wrap;\n        justify-content: space-around;\n    }\n\n    &__item {\n        width: calc(50% - 16px);\n        padding: 8px;\n    }\n}\n.contact {\n    &__container {\n        background-color: white;\n    }\n    &__link {\n        text-decoration: none;\n        color: black;\n        display: inline-block;\n        font-size: 32px;\n    }\n}\n.footer {\n    text-align: center;\n    background-color: black;\n    color: white;\n}"},"lineCount":null}},"error":null,"hash":"92de8ffc15ae655fb1d583fb59ee042d","cacheData":{"env":{}}}